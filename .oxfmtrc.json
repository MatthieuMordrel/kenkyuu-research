{
  "$schema": "./node_modules/oxfmt/configuration_schema.json",
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": false,
  "trailingComma": "es5",
  "ignorePatterns": [
    "node_modules",
    "dist",
    "build",
    ".output",
    "*.gen.ts",
    "bun.lock"
  ]
}
